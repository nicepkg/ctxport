{
  "name": "@ctxport/shared-ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "description": "Shared UI components for ctxport web and extension apps",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts",
      "import": "./dist/index.js"
    },
    "./components/ui": {
      "types": "./dist/components/ui/index.d.ts",
      "development": "./src/components/ui/index.ts",
      "import": "./dist/components/ui/index.js"
    },
    "./components/common": {
      "types": "./dist/components/common/index.d.ts",
      "development": "./src/components/common/index.ts",
      "import": "./dist/components/common/index.js"
    },
    "./components/layout": {
      "types": "./dist/components/layout/index.d.ts",
      "development": "./src/components/layout/index.ts",
      "import": "./dist/components/layout/index.js"
    },
    "./components/renderer": {
      "types": "./dist/components/renderer/index.d.ts",
      "development": "./src/components/renderer/index.ts",
      "import": "./dist/components/renderer/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "development": "./src/hooks/index.ts",
      "import": "./dist/hooks/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "development": "./src/utils/index.ts",
      "import": "./dist/utils/index.js"
    },
    "./i18n": {
      "types": "./dist/i18n/index.d.ts",
      "development": "./src/i18n/index.ts",
      "import": "./dist/i18n/index.js"
    },
    "./i18n/core": {
      "types": "./dist/i18n/core.d.ts",
      "development": "./src/i18n/core.ts",
      "import": "./dist/i18n/core.js"
    },
    "./i18n/react": {
      "types": "./dist/i18n/react.d.ts",
      "development": "./src/i18n/react.tsx",
      "import": "./dist/i18n/react.js"
    },
    "./i18n/types": {
      "types": "./dist/i18n/types.d.ts",
      "development": "./src/i18n/types.ts",
      "import": "./dist/i18n/types.js"
    },
    "./styles/globals.css": "./src/styles/globals.css",
    "./styles/renderer.css": "./src/styles/renderer.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc -b --pretty false",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.33.0",
    "html-react-parser": "^5.2.16",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.563.0",
    "mermaid": "^11.12.2",
    "next-themes": "^0.4.6",
    "react-day-picker": "^9.13.0",
    "react-draggable": "^4.5.0",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^4.6.0",
    "react-zoom-pan-pinch": "^3.7.0",
    "recharts": "2.15.4",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.22.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@esbuild-plugins/tsconfig-paths": "^0.1.2",
    "@types/node": "catalog:tooling",
    "@types/react": "catalog:tooling",
    "@types/react-dom": "catalog:tooling",
    "tsup": "catalog:tooling",
    "typescript": "catalog:tooling",
    "vitest": "catalog:tooling"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
